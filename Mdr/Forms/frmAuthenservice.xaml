                <TextBlock x:Name="textBlock" Text="ประวัติการขอ Authen Code สปสช." Foreground="White" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="252,102,0,0" FontSize="20"/>
                <StackPanel x:Name="stackPanel" Orientation="Horizontal" Margin="0,141,10,0" HorizontalAlignment="Right">
                    <StackPanel Orientation="Vertical">
                        <GroupBox x:Name="gsmc" Header="รูปภาพ"  Height="135" Width="113">
                            <StackPanel Orientation="Vertical">
                                <Image x:Name="imgPerson" Height="90" Width="101"/>
                            </StackPanel>
                        </GroupBox>
                        <StackPanel Orientation="Horizontal">
                            <materialDesign:PackIcon Kind="User" VerticalAlignment="Center" Height="34" RenderTransformOrigin="0.5,0.5" Width="37" Foreground="White"/>
                            <hc:TextBox x:Name="txtpt_fullname"  BorderBrush="{x:Null}" BorderThickness="0,0,0,2" Width="210" Margin="5,11,0,3" FontSize="17" Height="45" Background="{x:Null}" HorizontalAlignment="Stretch" Text="นายมนูญศักดิ์ สิทธิดำรงค์" Foreground="White"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <materialDesign:PackIcon Kind="UserCardDetails" VerticalAlignment="Center" Height="34" RenderTransformOrigin="0.5,0.5" Width="37" Foreground="White"/>
                            <hc:TextBox x:Name="txtpt_cid"  BorderBrush="{x:Null}" BorderThickness="0,0,0,2" Width="210" Margin="5,0,0,3" FontSize="17" Height="45" Background="{x:Null}" HorizontalAlignment="Stretch" Text="XXXXXXXXXXXXX" Foreground="White"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <materialDesign:PackIcon Kind="Telephone" VerticalAlignment="Center" Height="34" RenderTransformOrigin="0.5,0.5" Width="37" Foreground="White"/>
                            <hc:TextBox x:Name="txtpt_phone"  BorderBrush="{x:Null}" BorderThickness="0,0,0,2" Width="210" Margin="5,0,0,0" FontSize="17" Height="45" Background="{x:Null}" HorizontalAlignment="Stretch" Text="0981234567" Foreground="White"/>
                        </StackPanel>
                    </StackPanel>
                    <DataGrid x:Name="dataGridAuthen" d:ItemsSource="{d:SampleData ItemCount=5}" Background="{x:Null}" RowHeaderWidth="60" AutoGenerateColumns="False" Width="625" LoadingRow="dataGridAuthen_LoadingRow">
                        <DataGrid.Columns>
                            <DataGridTextColumn Header="claimType"   Binding="{Binding claimType}" />
                            <DataGridTextColumn Header="claimCode"  Binding="{Binding claimCode}"/>
                            <DataGridTextColumn Header="hcode"  Binding="{Binding hcode}"/>
                            <DataGridTextColumn Header="claimDateTime"  Binding="{Binding claimDateTime}"/>
                        </DataGrid.Columns>
                    </DataGrid>
                </StackPanel>




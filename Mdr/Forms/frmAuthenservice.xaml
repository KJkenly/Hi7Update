                <Label x:Name="label" Content="X" HorizontalAlignment="Left" Margin="1000,95,0,0" VerticalAlignment="Top" Foreground="White" Background="{x:Null}" BorderBrush="{x:Null}" FontSize="24" MouseLeftButtonDown="Label_MouseLeftButtonDown" Cursor="Hand"/>

